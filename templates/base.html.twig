<script src="{{ asset('assets/plugins/flot-old/jquery.flot.pie.min.js') }}"></script> #}

<script src="{{ asset('assets/plugins/bs-custom-file-input/bs-custom-file-input.min.js') }}"></script>
<script>
    $(function () {
        bsCustomFileInput.init()
    })
</script>

{% block script %}

{% endblock %}
</body>
</html>
